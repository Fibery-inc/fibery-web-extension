{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Fibery web extension",
  "icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48": "favicon48.png",
    "128": "favicon128.png"
  },
  "action": {
    "default_icon": {
      "16": "favicon16.png",
      "32": "favicon32.png",
      "48": "favicon48.png",
      "128": "favicon128.png"
    },
    "default_popup": "index.html",
    "default_title": "Fibery web extension"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*.fibery.io/*"
  ]
}

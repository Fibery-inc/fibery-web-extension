{
  "manifest_version": 3,
  "version": "1.0.7",
  "name": "Fibery Browser Extension",
  "description": "Use Fibery Extension to create Entities — Competitors, Pages, Tickets — right from a web page",
  "icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48": "favicon48.png",
    "128": "favicon128.png"
  },
  "commands": {
    "_execute_action": {
      "description": "Save to Fibery ",
      "suggested_key": {
        "chromeos": "Ctrl+Shift+K",
        "linux": "Ctrl+Shift+K",
        "mac": "Command+Shift+K",
        "windows": "Ctrl+Shift+K"
      }
    }
  },
  "action": {
    "default_icon": {
      "16": "favicon16.png",
      "32": "favicon32.png",
      "48": "favicon48.png",
      "128": "favicon128.png"
    },
    "default_popup": "index.html",
    "default_title": "Save to Fibery"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*.fibery.io/*"
  ]
}

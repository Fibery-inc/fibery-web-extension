{
  "name": "fibery-web-extension",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">16.7"
  },
  "dependencies": {
    "@types/node": "^16.9.3",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-query": "3.24.2",
    "react-scripts": "4.0.3",
    "typescript": "~4.4.3"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "release": "rm -rf dist && rm -rf release && mkdirp release && mkdirp release/chrome && mkdirp release/firefox && VITE_APP_HOST=https://fibery.io yarn build && node make-release.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.3",
    "@vitejs/plugin-react": "1.0.9",
    "autoprefixer": "10.4.0",
    "dotenv": "10.0.0",
    "postcss": "8.3.11",
    "prettier": "2.4.1",
    "tailwindcss": "2.2.19",
    "vite": "2.6.14"
  }
}
